    "native": {
        "ip": "0.0.0.0",
        "port": 54321,
        "ownPort": 53423,
        "pingInterval": 20000,
        "token": "FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF"
    },
    "instanceObjects": [
        {
            "_id": "info",
            "type": "channel",
            "common": {
                "name": "Information"
            },
            "native": {}
        },
        {
            "_id": "info.connection",
            "type": "state",
            "common": {
                "role": "indicator.connected",
                "name": "If connected to Plug",
                "type": "boolean",
                "read": true
            },
            "native": {}
        },
        {
            "_id": "power",
            "type": "state",
            "common": {
                "name": "Plug power",
                "type": "boolean",
                "read": true,
                "write": true,
                "role": "switch"
            },
            "native": {}
        },
        {
            "_id": "temperature",
            "type": "state",
            "common": {
                "name": "Plug temperature",
                "type": "number",
                "read": true,
                "write": false,
                "unit": "°C",
                "role": "value.temperature"
            },
            "native": {}
        },
        {
            "_id": "wifi_led",
            "type": "state",
            "common": {
                "name": "Plug wifi LED",
                "type": "boolean",
                "read": true,
                "write": true,
                "role": "switch"
            },
            "native": {}
        }
    ]
}
